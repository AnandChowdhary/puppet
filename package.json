{
  "name": "puppet",
  "version": "1.4.0",
  "description": "Natural-language web automation using Puppeteer",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand --forceExit",
    "test-report": "jest --forceExit --runInBand --coverage --coverageReporters=text-lcov | coveralls",
    "test-without-reporting": "jest --forceExit --runInBand --coverage",
    "semantic-release": "semantic-release",
    "build-website": "parcel build website/index.html",
    "start-website": "parcel website/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnandChowdhary/puppet.git"
  },
  "keywords": [
    "node.js",
    "typescript",
    "javascript",
    "library",
    "puppeteer",
    "scraping",
    "automation"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnandChowdhary/puppet/issues"
  },
  "homepage": "https://anandchowdhary.github.io/puppet/",
  "dependencies": {
    "@medv/finder": "^1.1.2",
    "fs-extra": "^9.0.0",
    "got": "^10.7.0",
    "ms": "^2.1.2",
    "natural": "^0.6.3",
    "puppeteer": "^2.1.1",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/ms": "^0.7.31",
    "@types/natural": "^0.6.3",
    "@types/node": "^13.11.1",
    "@types/puppeteer": "^2.0.1",
    "@types/signale": "^1.4.1",
    "coveralls": "^3.0.11",
    "jest": "^25.3.0",
    "parcel": "^1.12.4",
    "sass": "^1.26.3",
    "semantic-release": "^17.0.4",
    "semantic-release-gitmoji": "^1.3.3",
    "ts-jest": "25.2.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
