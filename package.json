{
  "name": "puppet",
  "version": "1.1.0",
  "description": "Natural-language web automation using Puppeteer",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-report": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test-without-reporting": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnandChowdhary/puppet.git"
  },
  "keywords": [
    "node.js",
    "typescript",
    "javascript",
    "library",
    "puppeteer",
    "scraping",
    "automation"
  ],
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnandChowdhary/puppet/issues"
  },
  "homepage": "https://anandchowdhary.github.io/puppet/",
  "dependencies": {
    "fs-extra": "^9.0.0",
    "got": "^10.7.0",
    "puppeteer": "^2.1.1",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/puppeteer": "^2.0.1",
    "@types/signale": "^1.4.1",
    "coveralls": "^3.0.11",
    "jest": "^25.3.0",
    "semantic-release": "^17.0.4",
    "semantic-release-gitmoji": "^1.3.3",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
